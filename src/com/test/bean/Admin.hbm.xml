<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.test.bean.Admin" table="admins">
		
		<id name="adminid" column="adminid" type="int">
			<generator class="increment"> <!-- 主键id的生成方式为自增 -->
			</generator>
		</id>
		
		<property name="adminname" column="adminname" type="string"></property>
		<property name="adminpassword" column="adminpassword" type="string"></property>
	</class>

</hibernate-mapping>

